{
    "Version": "2012-10-17",
    "Statement": [
        {
            "Sid": "CloudWatchCreateLogGroup",
            "Effect": "Allow",
            "Action": "logs:CreateLogGroup",
            "Resource": "arn:aws:logs:sa-east-1:596533897380:*"
        },
        {
            "Sid": "CloudWatchWriteLogs",
            "Effect": "Allow",
            "Action": [
                "logs:CreateLogStream",
                "logs:PutLogEvents"
            ],
            "Resource": "arn:aws:logs:sa-east-1:596533897380:log-group:/aws/lambda/*:*"
        }
    ]
}